{
  "name": "uchet",
  "author": "Musyl Nikita",
  "description": "uchet",
  "version": "0.0.1",
  "private": true,
  "repository": "https://github.com/JohnDorian00/uchet",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "dist:win32": "electron-builder --win --ia32",
    "rebuild-electron": "electron-rebuild -f -w sqlite3",
    "electron:build": "vue-cli-service electron:build --win --ia32",
    "electron:packagerLinux": "electron-packager . --platform=linux --arch=ia32 --overwrite uchet --no-prune",
    "electron:serve": "vue-cli-service electron:serve --win --ia32",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "ag-grid-community": "^25.1.0",
    "ag-grid-vue": "^25.1.0",
    "aws-sdk": "^2.889.0",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "idb": "^6.0.0",
    "jquery": "^3.6.0",
    "jqwidgets-scripts": "^12.0.4",
    "node-pre-gyp": "^0.11.0",
    "smart-webcomponents": "^9.1.0",
    "sqlite3": "^4.2.0",
    "v-blur": "^1.0.4",
    "vue": "^2.6.12",
    "vue-notification": "^1.3.20",
    "vue-property-decorator": "^8.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "electron": "^11.4.3",
    "electron-devtools-installer": "^3.1.0",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.6",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-constant-condition": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
